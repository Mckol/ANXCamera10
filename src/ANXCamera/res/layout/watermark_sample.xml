            <TextView android:textSize="@dimen/extend_time_watermark_1_month_text_size" android:textColor="#b2ffffff" android:gravity="center" android:id="@id/extend_month" android:layout_width="@dimen/extend_time_watermark_1_month_width" android:layout_height="@dimen/extend_time_watermark_1_month_height" android:layout_marginTop="@dimen/extend_time_watermark_1_month_margin_top" android:fontFamily="mipro-light" android:letterSpacing="0.1" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:gravity="center" android:layout_gravity="center" android:id="@id/extend_time_watermark_2" android:visibility="invisible" android:layout_width="@dimen/extend_time_watermark_2_width" android:layout_height="@dimen/extend_time_watermark_2_height">
        <LinearLayout android:gravity="center_horizontal" android:layout_gravity="center" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
            <TextView android:textSize="@dimen/extend_time_watermark_2_day_text_size" android:textColor="#ffffffff" android:gravity="center" android:id="@id/extend_day" android:layout_width="@dimen/extend_time_watermark_2_day_width" android:layout_height="@dimen/extend_time_watermark_2_day_height" android:fontFamily="mitype-heavy" android:textAlignment="center" />
            <TextView android:textSize="@dimen/extend_time_watermark_2_week_text_size" android:textColor="#ffffffff" android:gravity="center" android:id="@id/extend_week" android:layout_width="@dimen/extend_time_watermark_2_week_width" android:layout_height="@dimen/extend_time_watermark_2_week_height" android:fontFamily="mipro-light" android:letterSpacing="0.1" />
            <TextView android:textSize="@dimen/extend_time_watermark_2_logo_text_size" android:textColor="#99ffffff" android:gravity="center" android:layout_width="@dimen/extend_time_watermark_2_logo_width" android:layout_height="@dimen/extend_time_watermark_2_logo_height" android:layout_marginTop="@dimen/extend_time_watermark_2_logo_margin_top" android:text="@string/extend_time_watermark_2_logo_text" android:fontFamily="mipro-light" android:letterSpacing="0.4" />
        </LinearLayout>
